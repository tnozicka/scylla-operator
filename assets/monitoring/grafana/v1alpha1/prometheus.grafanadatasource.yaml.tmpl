apiVersion: integreatly.org/v1alpha1
kind: GrafanaDataSource
metadata:
  name: {{ .Name }}
spec:
  name: middleware.yaml
  datasources:
  - name: prometheus
    type: prometheus
    access: proxy
    url: http://172.17.0.15:9090
    isDefault: true
    version: 1
    editable: true
    jsonData:
      tlsSkipVerify: true
      timeInterval: "5s"
